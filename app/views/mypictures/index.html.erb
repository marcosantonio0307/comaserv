<div class="row">
	<%= render '/menu' %>
</div>

<div class="image-back background">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 box-center">
				<h1>NOSSOS EVENTOS</h1><br>
				<p align="center">Veja algumas fotos de eventos realizados por nossa equipe!</p>
			</div>
		</div><br>
		<div class="row">
			<div class="col-lg-12">
				<% @pictures.each do |picture| %>
					<%= link_to mypicture_path(picture) do %>
						<div class="col-lg-5 col-md-5 col-sm-12 box-3">
							<div>
								<%= image_tag picture.files.first %>
								<h4><%= picture.name %></h4>
							</div>
						</div>
					<% end %>
				<% end %>
			</div>
		</div>
	</div>
</div>