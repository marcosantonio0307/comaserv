<%= render '/admin/logobar' %>

<div class="row">
	<div class="">
		<div class="col-lg-12 col-md-12 col-sm-12">
			<div class="form-center">
				<h2><b>INCLUIR FOTOS NO ALBUM: <%= @picture.name %></b></h2><br><br>

				<%= form_with model: @picture, url: mypicture_file_uploads_path(@picture),
				              method: :post, local: true do |f| %>
				    <div class="form-group">
				    	<%= f.file_field :files, multiple: true %><br>
				  		<%= f.submit 'Incluir', class: 'btn btn-success' %>
				    </div>
				  
				<% end %>
			</div>
		</div>
	</div> 
</div>
 
